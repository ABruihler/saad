{
  "readFile": {
    "command": "cat {path}"
  },
  "reverse": {
    "command": "echo {string} | rev"
  },
  "checkComplexity": {
    "command": "python scripts/probes/complexity.py {path} {target} {threshold}"
  },
  "fileChange": {
    "command": "python3 scripts/probes/filechange.py {HEAD}/{file} {HEAD~1}/{file}"
  },
  "slackBot": {
    "command": "if [ \"{condition}\" = \"True\" ]; then python3.8 scripts/actuators/slack_actuator.py $(cat scripts/actuators/slackbot_token.txt) {channel} {message} {blocks}; fi"
  },
  "pythonAST": {
    "command": "python3 scripts/target_finders/python_ast.py {file} {astLocation}"
  }
}
